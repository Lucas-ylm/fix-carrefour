{
  "name": "boilerplate-mpublicite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./out",
    "dev": "next dev",
    "build-next": "next build",
    "build:prod": "cross-env BUILD_DEST=production npm-run-all --serial clean build-next optimize",
    "build:test": "cross-env BUILD_DEST=preview npm-run-all --serial clean build-next optimize",
    "optimize": "tsx ./optimize-images",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint-staged": "lint-staged",
    "format": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18",
    "react-use-measure": "^2.1.1",
    "framer-motion": "^11.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@svgr/webpack": "^8.1.0",
    "@types/google-publisher-tag": "^1.20241104.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "conventional-changelog-atom": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "eslint-plugin-import": "^2.31.0",
    "glob": "^10.3.15",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7",
    "sass": "^1.79.4",
    "sharp": "^0.33.3",
    "tsx": "^4.10.2",
    "typescript": "^5"
  }
}
